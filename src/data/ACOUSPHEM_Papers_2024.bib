@article{Jou17b,
  author   = {Nori, M. and Venegas, R.},
  title    = {Sound propagation in porous materials with annular pores},
  journal  = {The Journal of the Acoustical Society of America},
  year     = {2017},
  volume   = {141},
  number   = {6},
  pages    = {4642-4651},
  abstract = {Long-wavelength sound propagation in porous materials with annular pores
              is investigated in this paper. Closed-form analytical expressions for
              the effective acoustical properties of this type of material were
              obtained. These are compared with both direct numerical calculations of
              the effective properties and their calculations obtained by using
              semi-phenomenological models. Analytical expressions for the input
              parameters of the latter, i.e., static viscous and thermal
              permeabilities, viscous and thermal characteristic lengths, and
              tortuosity, are also provided. The introduced model is successfully
              validated by comparing its predictions with measured data taken from
              literature. A parametric analysis that allows highlighting the influence
              of the different geometrical parameters of porous materials with annular
              pores on their sound absorptive properties is also presented. (C) 2017
              Acoustical Society of America.},
  url      = {https://doi.org/10.1121/1.4986939}
}
% Month = {JUN},

