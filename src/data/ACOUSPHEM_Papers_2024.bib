@article{Jou18c,
  author   = {Venegas, R. and Arenas, J. P. and Boutin, C.},
  title    = {Analytical modeling of dissipative silencers},
  journal  = {The Journal of the Acoustical Society of America},
  year     = {2018},
  volume   = {144},
  number   = {5},
  pages    = {2998-3009},
  abstract = {This paper applies ideas originally developed in the field of acoustics
              of multiscale porous media to the analytical modeling of dissipative
              silencers. Analytical models for the long-wavelength acoustical
              properties of different types of dissipative silencers, such as pod
              silencers, lined ducts, splitters, and bar silencers, are introduced in
              this work. Some of these models account for the possibility of using
              different porous materials in a given silencer. For instance, a model
              for a splitter silencer comprising baffles made of different porous
              materials is derived. Examples of the sound transmission behavior of the
              said types of silencers are presented to evidence that the dissipation
              of sound energy is largely influenced by pressure diffusion occurring in
              the porous constituents. The predictions of the analytical models are in
              good agreement with finite element calculations. (C) 2018 Acoustical
              Society of America.},
  doi      = {10.1121/1.5078607}
}
